{
    "name": "josipjelic/eav-bundle",
    "type": "symfony-bundle",
    "description": "Basic entity-attribute-value management (the `EAV` domain)",
    "keywords": [
        "entity-attribute-value",
        "eav"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/josipjelic/msgphp-eav.git",
            "canonical": false
        },
        {
            "type": "vcs",
            "url": "https://github.com/josipjelic/msgphp-domain.git",
            "canonical": false
        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "josipjelic/msgphp-eav": "^0.15",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.15-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\EavBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
